}
.learnListItem_img{
  width: 710rpx;
  height: 290rpx;
  display: block;
  margin: 0 auto;
}
.learnListItem_tittle{
  width:710rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(60,60,60,1);
  line-height:42rpx;
  margin: 0 auto;
  padding-top: 20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
