.swiperBox{
  width: 100%;
  height: 378rpx;
}
.swiperItem{
  width: 100%;
  height: 100%;
}
.swiperBox_img{
  width: 100%;
  height: 100%;
}
.learnList{
  overflow: hidden;
}
.learnListItem{
  width:100%;
  background:rgba(255,255,255,1);
  padding: 20rpx 0;
