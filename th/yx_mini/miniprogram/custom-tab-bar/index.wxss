.tabbar {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  background-color: #fff;
  display: flex;
  justify-content:space-between;
  align-items: center;
  padding: 10rpx 50rpx 10rpx;
  box-sizing: border-box;
  color: #ACACAC;
  text-align: center;
}
.item {
  width: 200rpx;
  height: 88rpx;
}
.item-img{
  display: block;
  margin: 0 auto;
  width: 48rpx;
  height: 48rpx;
}
.item-text{
  line-height: 20rpx;
  font-size: 20rpx;
  margin-top: 10rpx;
}
.big-img {
  width: 88rpx;
  height: 88rpx
}
button{
  display: block;
  border: none;
  outline: none;
  background: none;
}
button::after {
  border: none
}
