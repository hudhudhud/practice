.date-box {
position: relative;
  width: 100%;
  padding-bottom: 20rpx;
  background-color: #3f90fd;
  background: linear-gradient(-53deg, rgba(88, 81, 245, 0.74), rgba(122, 109, 254, 0.74));
  color: #fff;
  font-size: 30rpx;
}

/* 标题 */

.data-title {
  height: 129rpx;
  line-height: 129rpx;
  margin: 0 72rpx;
  color: rgba(255, 255, 255, 0.6);
  font-size: 26rpx;
}

.title-date {
  font-size: 40rpx;
  color: #fff;
  margin-right: 21rpx;
}

.title-week {
  float: right;
}

/* 星期 文字 */

.weeks-text {
  margin: 0 auto;
  width: 690rpx;
  height: 70rpx;
  overflow: hidden;
  font-size: 0;
  white-space: nowrap;
}

.week-text {
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  order: 2;
  text-align: center;
  margin: 0 15rpx;
  font-size: 30rpx;
}

.week-text.first {
  float: left;
}

/* 日期 item */

.days {
  font-size: 0;
  margin: 0 auto;
  width: 690rpx;
}

.day {
  border-radius: 50%;
  display: inline-block;
  text-align: center;
  line-height: 70rpx;
  width: 70rpx;
  height: 70rpx;
  margin: 2rpx 15rpx;
  font-size: 30rpx;
}

.day.active {
  background-color: #ffd200;
  background: linear-gradient(106deg, rgba(254, 191, 0, 1), rgba(255, 235, 0, 1));
  color: #3f90fd;
}
.day.disabled {
  background-color: gainsboro!important;
   background: gainsboro!important;
  color: #fff!important;
}







.swiper {
  min-height: 450rpx;
}

/* 图标 */

.icon-right {
   z-index: 100;
  position: absolute;
  right: 0;
  top: 320rpx;
  padding-right: 20rpx;
}
.icon-right .icon-img{
 width: 16rpx;
  height: 35rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

.icon-left {
  z-index: 100;
  position: absolute;
  left: 0;
  top: 320rpx;
  padding-left: 20rpx;
}


.icon-left .icon-img{
 width: 16rpx;
  height: 35rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  transform: rotate(180deg);
}
